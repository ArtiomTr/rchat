{
	"$schema": "https://turborepo.org/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"cache": true,
			"dependsOn": ["^build"],
			"outputs": ["prepared-package"],
			"inputs": ["src/**/*.tsx", "src/**/*.ts", "package.json"]
		},
		"test": {
			"cache": true,
			"dependsOn": [],
			"outputs": [],
			"inputs": ["src/**/*.tsx", "src/**/*.ts", "tests/**/*.ts", "tests/**/*.tsx"]
		},
		"lint": {
			"outputs": []
		}
	}
}
